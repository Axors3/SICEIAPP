pipeline{
    agent any
    stages{
        stage('Build') {
            steps {
                bat 'npm install'
            }
        }
        stage('Build Image') {
            steps {
                script {
                    docker build -t siceiapp-master .
                    docker run -dp 3000:3000 siceiapp-master 
                }
            }
        }
        
    }
}