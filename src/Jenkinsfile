pipeline{
    agent any
    stages{
        stage('Build') {
            steps {
                bat 'npm install'
            }
        }
        stage('Build Image') {
            steps {
                script {
                    docker build -t siceiapp-master:1.0.0-10 .
                    docker run -it siceiapp-master:1.0.0-10 cmd
                }
            }
        }
        
    }
}